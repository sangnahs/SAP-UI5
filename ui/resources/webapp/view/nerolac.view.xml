<mvc:View controllerName="ui.controller.nerolac" displayBlock="true" id="nerolacid" xmlns:html="http://www.w3.org/1999/xhtml" 
    xmlns:mvc="sap.ui.core.mvc" 
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form">
	<App>
		<pages>
            <Page id="mainpageid" title="Paints" class="sapUiStdPage" showNavButton="true"
				  navButtonText="Back" navButtonPress="onPressDetailBack">
				<content>
				
		  <TileContainer
			id="container">
			<StandardTile 
				icon="{image}"
				title="{paint_name}"
				press="onpress"/>
		</TileContainer>
				</content>
			</Page>

		</pages>
	</App>
</mvc:View>

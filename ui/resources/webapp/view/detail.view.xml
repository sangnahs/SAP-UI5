<mvc:View controllerName="ui.controller.detail" displayBlock="true" id="Detailid" xmlns:html="http://www.w3.org/1999/xhtml" 
    xmlns:mvc="sap.ui.core.mvc" 
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form">
	<App>
		<pages>
            <Page id="mainpageid" title="Paints" class="sapUiStdPage" showNavButton="true"
				  navButtonText="Back" navButtonPress="onPressDetailBack">
				<content>
				
		  <TileContainer
			id="container"
			 
			tiles="{path :'model1>/all_paint'}">
			<StandardTile
			    icon="{model1>image}"
				title="{model1>paint_name}"
				press="onpress"
				 />
		</TileContainer>
				</content>
			</Page>

		</pages>
	</App>
</mvc:View>

